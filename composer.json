{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "brainmaestro/composer-git-hooks": "v3.0.0-alpha.1",
        "doctrine/dbal": "^3.3",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.7",
        "pestphp/pest": "^1.21"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "enlightn/security-checker": "^1.10",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest-plugin-laravel": "^1.2",
        "pestphp/pest-plugin-parallel": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cghooks": "[ -f vendor/bin/cghooks ] && vendor/bin/cghooks update || echo 'cghooks not found'",
        "ide-helper": [
            "@php artisan ide-helper:generate -n",
            "@php artisan ide-helper:meta -n",
            "@php artisan ide-helper:models -n"
        ],
        "security-checker": "vendor/bin/security-checker security:check",
        "phpunit": "vendor/bin/phpunit --color=always",
        "pest": "vendor/bin/pest --color=always",
        "chmoder" : "chmod -R 777 storage bootstrap && chown -R 1001:1001 \"/root\"",
        "clear": [
            "@php artisan config:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan clear-compiled",
            "@chmoder"
        ],
        "cache-clear": [
            "@php artisan cache:clear"
        ],
        "dev-setup": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@putenv CACHE_DRIVER=file",
            "@chmoder",
            "@clear",
            "@cache-clear",
            "@php artisan key:generate",
            "@php artisan migrate --seed",
            "@ci",
            "@cghooks",
            "@ide-helper"
        ],
        "ci": [
            "chmod -R 777 storage bootstrap",
            "@tests"
        ],
        "pre-commit": [
            "@composer run security-checker"
        ],
        "pre-push": [
            "@composer run pest"
        ],
        "tests": [
            "@pre-commit",
            "@pre-push"
        ],
        "post-merge": [
            "@php artisan migrate",
            "composer install",
            "echo \"\nSeed, if in need: \\e[32mphp artisan db:seed\\e[0m\"\n"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
